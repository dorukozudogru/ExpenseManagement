    <script>
        $(document).ready(function () {
            $('#allTables').DataTable({
                "language": {
                    processing: '<i class="fas fa-sync fa-spin fa-3x fa-fw"></i><span class="sr-only">Yükleniyor...</span> ',
                    zeroRecords: "Hiçbir kayıt bulunamadı.",
                    emptyTable: "Kayıt bulunmamaktadır.",
                    search: "Ara:",
                    oPaginate: {
                        "sNext": "»",
                        "sPrevious": "«"
                    },
                    sLengthMenu: "Listelenen maksimum kayıt sayısı: _MENU_",
                    sInfo: "Toplam _TOTAL_ kayıttan _START_ - _END_ arası gösteriliyor",
                },
                "pageLength": 100
            });

            bsCustomFileInput.init();
        });

        $('.select2').select2()

        $('#datepicker').datepicker({
            autoclose: true,
            format: "yyyy/mm/dd",
            todayHighlight: true,
            orientation: "bottom auto"
        })

        $('#datepicker2').datepicker({
            autoclose: true,
            format: "yyyy/mm/dd",
            todayHighlight: true,
            orientation: "bottom auto"
        })

        $('.swalDefaultSuccess').click(function () {
            Swal.fire({
                position: 'center',
                icon: 'success',
                title: document.getElementById("swalDefaultSuccess").value,
                showConfirmButton: false,
                timer: 1500
            })
        });

        $('.swalDefaultWarning').click(function () {
            Swal.fire({
                title: 'Emin misiniz?',
                text: document.getElementById("swalDefaultWarning").value,
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: document.getElementById("warningSuccessMessage").value,
                cancelButtonText: 'Geri Dön'
            }).then((result) => {
                if (result.dismiss == "cancel") {
                    window.history.back();
                }
                if (result.value && document.getElementById("warningSuccessMessage").value == "Devam Et") {
                    Swal.fire({
                        title: 'Unutmayın!',
                        text: 'Alış tarihinde yapacağınız değişiklik satış kaydını da etkileyecektir.',
                        icon: 'warning',
                        confirmButtonColor: '#3085d6',
                        confirmButtonText: "Anladım!"
                    })
                }
            })
        });

        $('.swalDefaultError').click(function () {
            Swal.fire({
                position: 'center',
                icon: 'error',
                title: document.getElementById("swalDefaultError").value,
                showConfirmButton: false,
                timer: 1500
            })
        });

        $("#resultRow").hide();
        $("#exportButton").hide();
    </script>
    <script type='text/javascript'>
        var url = window.location;
        const allLinks = document.querySelectorAll('.nav-item a');
        const currentLink = [...allLinks].filter(e => {
            return e.href == url;
        });

        if (currentLink[0] != null) {
            currentLink[0].classList.add("active");
            if (currentLink[0].closest(".nav-treeview") != null) {
                currentLink[0].closest(".nav-treeview").style.display = "block";
                currentLink[0].closest(".has-treeview").classList.add("active");
                currentLink[0].parentElement.parentElement.parentElement.children[0].classList.add("active");
                currentLink[0].parentElement.parentElement.parentElement.classList.add("menu-open");
                if (currentLink[0].parentElement.parentElement.parentElement.parentElement.parentElement != null) {
                    currentLink[0].parentElement.parentElement.parentElement.parentElement.parentElement.children[0].classList.add("active");
                    currentLink[0].parentElement.parentElement.parentElement.parentElement.parentElement.classList.add("menu-open");
                }
            }
        }
    </script>
